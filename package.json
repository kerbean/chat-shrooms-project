  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "axios": "^0.18.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "firebase": "^7.10.0",
    "if-env": "^1.0.4",
    "json-find": "^2.0.7",
    "mongoose": "^5.3.16"
  }
}
